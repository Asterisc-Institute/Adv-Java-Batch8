package model;

public class Employee {
	
	private int id;
	private String name;
	private Address add;
	
	public Employee(int id) {
		super();
		this.id = id;
	}
	public Employee(String name) {
		super();
		this.name = name;
	}
	
	public Employee(Address add) {
	
		this.add = add;
	}
	
	public Employee(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	public Employee(int id, Address add) {
		super();
		this.id = id;
		this.add = add;
	}
	
	public Employee(int id, String name, Address add) {
		super();
		this.id = id;
		this.name = name;
		this.add = add;
	}
	
	
	

}
